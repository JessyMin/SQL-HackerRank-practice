#  2018.3.18

# 옳은 쿼리
SELECT city
  , CHAR_LENGTH(city)
FROM station
WHERE CHAR_LENGTH(city) = (SELECT MIN(CHAR_LENGTH(city)) FROM station)
ORDER BY city
LIMIT 1;


# 잘못된 쿼리
SELECT city
  , CHAR_LENGTH(city)
FROM station
WHERE CHAR_LENGTH(city) = MAX(CHAR_LENGTH(city)
ORDER BY city
LIMIT 1;

# ERROR 1111 (HY000) at line 1: Invalid use of group function
